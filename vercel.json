{
  "version": 2,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/server.js"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/daily-reminders",
      "schedule": "0 18 * * *"
    },
    {
      "path": "/api/cron/reset-weekly-xp",
      "schedule": "0 0 * * 1"
    }
  ]
}


